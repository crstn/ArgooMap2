package com.mapitz.gwt.googleMaps.client;

class GLatLngImpl {

    public static native JSObject create(double lat, double lng)/*-{
     return new $wnd.GLatLng(lat, lng);
     }-*/;

    public static native JSObject create(double lat, double lng,
            boolean unbounded)/*-{
     return new $wnd.GLatLng(lat, lng, unbounded);
     }-*/;

    public static native double distanceFrom(JSObject latlng, JSObject other)/*-{
     return latlng.distanceFrom(other);
     }-*/;

    public static native boolean equals(JSObject latlng, JSObject other)/*-{
     return latlng.equals(other);
     }-*/;

    public static native double lat(JSObject latlng)/*-{
     return latlng.lat();
     }-*/;

    public static native double latRadians(JSObject latlng)/*-{
     return latlng.latRadians();
     }-*/;

    public static native double lng(JSObject latlng)/*-{
     return latlng.lng();
     }-*/;

    public static native double lngRadians(JSObject latlng)/*-{
     return latlng.lngRadians();
     }-*/;

    public static native String toUrlValue(JSObject latlng)/*-{
     return latlng.toUrlValue();
     }-*/;

    public static native String toString(JSObject latlng)/*-{
     return latlng.toString();
     }-*/;

}
