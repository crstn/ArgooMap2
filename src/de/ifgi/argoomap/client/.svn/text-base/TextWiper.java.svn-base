package de.ifgi.argoomap.client;

import com.google.gwt.user.client.ui.ClickListener;
import com.google.gwt.user.client.ui.TextBox;
import com.google.gwt.user.client.ui.Widget;

/**
 * Use this class to wipe text from TextBoxes upon click. 
 * 
 * @author Carsten Keﬂler, carsten.kessler@uni-muenster.de
 * @version 12.04.2007
 *
 */
public class TextWiper implements ClickListener {
    public void onClick(Widget sender) {
            ((TextBox)sender).setText("");
    }
}
